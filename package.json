{
  "name": "gapi-to-graphql",
  "author": "rlancer",
  "license": "MIT",
  "version": "1.0.10",
  "description": "Converts any Google Data API to GraphQL",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": "https://github.com/rlancer/gapi-to-graphql",
  "email": "files.robert.lancer@gmail.com",
  "scripts": {
    "precommit": "npm run build && lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scratch": "ts-node src/scratch.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "download-apis": "ts-node src/google_apis/_download_all_gapis.ts"
  },
  "dependencies": {
    "@types/graphql": "^14.0.0",
    "@types/node": "^10.9.4",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "graphql": "^14.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "1.14.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "lint-staged": {
    "/src/**/*.js": [
      "prettier --print-width 150 --no-semi --single-quote --jsx-bracket-same-line --write",
      "git add"
    ]
  }
}
