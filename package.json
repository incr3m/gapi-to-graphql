{
  "name": "gapi-to-graphql",
  "author": "rlancer",
  "license": "MIT",
  "version": "1.0.6",
  "description": "Converts any Google Data API to GraphQL",
  "main": "dist/index.js",
  "repository": "https://github.com/rlancer/gapi-to-graphql",
  "email": "files.robert.lancer@gmail.com",
  "scripts": {
    "precommit": "npm run build && lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist",
    "prepublishOnly": "npm run build",
    "watch": "npm-watch",
    "dev": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "graphql": "^0.11.7",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "npm-watch": "^0.2.0",
    "prettier": "1.6.1"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-decorators-legacy"
    ],
    "presets": [
      "stage-0",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "lint-staged": {
    "/src/**/*.js": [
      "prettier --print-width 150 --no-semi --single-quote --jsx-bracket-same-line --write",
      "git add"
    ]
  }
}
